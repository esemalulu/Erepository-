<#if record.item?has_content>
<table class="itemtable" style="width: 100%; table-layout:relative;">	<!-- start items --><#list record.item as item><#if item_index==0>
<thead>
	<tr>
	<th align="left">Item</th>
	<th align="left">Description</th>
	<th align="right">Quantity</th>
	<th align="right">Rate</th>
	<th align="right">Amount</th>
	</tr>
</thead>
</#if>
 
	<tr>
		<td align="left">${item.item}</td>
		<td align="left">${item.description}</td>
		<td align="right">${item.quantity}</td>
		<td align="right">${item.rate}</td>
		<td align="right">${item.amount}</td>
	</tr>
    
	</#list>

	
</table>
<table class="total" style="width: 100%; margin-top:20px; font-size:12pt"><tr>
				<td colspan="4">&nbsp;</td>
				<td align="right"><b>Subtotal</b></td>
				<td align="right">${record.total}</td>
				</tr>			
				<tr class="totalrow">
				<td background-color="#ffffff" colspan="4">&nbsp;</td>
				<td align="right"><b>Total</b></td>
				<td align="right">${record.total}</td>
				</tr>
			</table>
</#if><br />
<br />

